{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "camera",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minProperties": 1
    },
    "imageUrl": {
      "type": "string"
    },
    "brand": {
      "type": "string",
      "minProperties": 1
    },
    "price": {
      "type": "string",
      "minProperties": 1,
      "format": "uri"
    },
    "connection": {
      "type": "string",
      "minProperties": 1
    },
    "model": {
      "type": "string",
      "minProperties": 1
    }
  },
  "required": ["name", "brand", "model", "connection", "price"],
  "additionalProperties": true
}
